/dts-v1/;
#include "sdp1501.dtsi"

/ {
	model = "Samsung DTV based on SDP1501 SoC";
	compatible = "samsung,dtv", "samsung,sdp1501", "samsung,sdp1501";

	memory {
		device_type = "memory";
		reg = <0x01000000 0x0FA00000>, <0x20000000 0x0B700000>, <0x45800000 0x0F200000>, <0x60000000 0x11700000>, <0x80000000 0x3B800000>;
	};

	chosen {
		/* BEWARE: DO_NOT_TOUCH___RESERVED_SPACE_50b _must_ be at the end of the
		 * cmdline, ALWAYS! This is the RULE!
		 *
		 * Violators will be shot!
		 * Survivors will be shot again!
		 * I am not kidding.
		 *                          -- roman.pen
		 */
		bootargs = __STR__(dtsversion=__DTS_VERSION__ root=/dev/mmcblk0p18 init=/sbin/init rootfstype=vdfs RO_PARAM console=ttyS0,115200N8 SELP_ENABLE=00000000 rootwait no_console_suspend earlyprintk sdp_sparsemem vmalloc=550M QUIET_PARAM TRACING_PARAM ROOT_PARAM DO_NOT_TOUCH___RESERVED_SPACE_50b);

		boot-modes = <&shift_s_mode>, <&normal_trace_mode>, <&shift_s_trace_mode>;
	};

	fixed-rate-clocks {
		fin {
			compatible = "samsung,sdp-clock-fin";
			clock-frequency = <24576000>;
		};
	};

	mmc {
		ddr50;
