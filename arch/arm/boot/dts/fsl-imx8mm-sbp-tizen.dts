#include "fsl-imx8mm-sbp-af.dts"

/ {
	tztv_kfactory {
		compatible = "samsung,tztv-kfactory";
		status = "okay";
	};

        tztv_system {
                compatible = "samsung,tztv-system";
		status = "okay";
	};

	tzav_jackident {
		compatible = "samsung,tzav-jackident";
		status = "okay";
	};
	
	hdmi_tx {
		compatible = "samsung,hdmi_tx";
		samsung,hdmi-tx0-hpd-con = <&gpio1 0 GPIO_ACTIVE_HIGH>;
		samsung,hdmi-audio-switch = <&gpio1 7 GPIO_ACTIVE_HIGH>;
		samsung,hdmi-tx-nreset = <&gpio1 11 GPIO_ACTIVE_HIGH>;
		status = "okay";
	};
};

&uart1 { /* micom */
	compatible = "samsung,uart-micom";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart1>;
	status = "okay";
};

&i2c1 {
	s24c512c@50 {
		compatible = "sii,s24c512c";
		pinctrl-0 = <&pinctrl_eeprom_wp>;
		reg = <0x50>;
		samsung,eeprom-wp = <&gpio1 1 GPIO_ACTIVE_HIGH>;
		status = "okay";
	};
};

&i2c2 {
	tas5747@22 {
		compatible = "tas,tas5747";
		reg = <0x22>;
		samsung,amp-nreset = <&gpio1 6 GPIO_ACTIVE_HIGH>;
		samsung,amp-wirelesstx-nreset = <&gpio1 4 GPIO_ACTIVE_HIGH>;
	};
};
