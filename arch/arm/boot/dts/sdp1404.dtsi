	timer: arch-timer {
		compatible = "arm,armv7-timer";
		interrupts = <1 14 0xf08>,
				<1 13 0xf08>;
	};

	timer@10090400 {
		compatible = "samsung,sdp-timer";
		reg = <0x10090400 0x100>;
		clocks = <&apb_pclk>;
		clock-names = "apb_pclk";
		int_stretch = <0xf>;
		int_level;
		one-shot;
		nr_timers = <6>;
		interrupts = <0 16 0>, <0 17 0>, <0 18 0>, <0 19 0>, <0 20 0>, <0 21 0>;
		clocksource_id = <0>;
		clockevent_id = <1>;
//		localtimer_ids = <2>, <3>, <4>, <5>;
	};




