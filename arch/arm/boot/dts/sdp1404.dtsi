	pci@0x111a0000 {
		compatible = "samsung,sdp-pcie", "sdp-pcie";
		clocks = <&pcie_mask>,<&pcie_clk >, <&pcie_gpr_clk>;
		clock-names = "pcie_mask","pcie_clk","pcie_gpr_clk";
