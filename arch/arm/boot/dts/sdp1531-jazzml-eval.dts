/dts-v1/;

#include "sdp1501.dtsi"

/ {
	model = "Samsung DTV Eval Board based on SDP1521(Jazz-ML) SoC";
	compatible = "samsung,dtv", "samsung,sdp1521";

	memory {
		device_type = "memory";
		reg = <0x43000000 0x0B200000>, <0x60000000 0x14000000>, <0x80000000 0x33600000>;
	};

	chosen {
		bootargs = "console=ttyS0,115200N8 root=/dev/mmcblk0p3 rootfstype=squashfs rootwait earlyprintk sdp_sparsemem vmalloc=700M";
	};

	fixed-rate-clocks {
		fin {
			compatible = "samsung,sdp-clock-fin";
			clock-frequency = <24576000>;
		};
	};
	
	mmc {
		hs400;
	};

	serial@00190A00 {
		status = "okay";
	};

	serial@00190A40 {
		status = "okay";
	};

	serial@00190A80 {
		status = "okay";
	};

	serial@00190AC0 {
		status = "okay";
	};

	pinctrl{
		compatible = "samsung,sdp-pinctrl";
		reg = <0x005C1000 0x312>;
		start-offset = <0x1b8>;
		nr-banks = <29>;
		model-sel = <1>;
	};
