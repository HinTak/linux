	dummy_panel {
			compatible = "dummy_panel";

			display-timings {
				native-mode = <&timing0>;

				timing0: timing-0 {
					clock-frequency = <0>;
					hactive = <1920>;
					vactive = <1080>;
					hfront-porch = <0>;
					hback-porch = <0>;
					hsync-len = <0>;
					vfront-porch = <0>;
					vback-porch = <0>;
					vsync-len = <0>;
			};
		};

	};

	ehci@00500000 {
		status = "okay";
	};

	ehci@00510000 {
		status = "okay";
	};

	ehci@00520000 {
		status = "okay";
	};

	ehci@00530000 {
		status = "okay";
	};


	xhci@00400000 {
		status = "okay";
	};


	ohci@00508000 {
		status = "okay";
	};

