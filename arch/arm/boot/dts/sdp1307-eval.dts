/dts-v1/;
#include "sdp1307.dtsi"

/ {
	model = "Samsung STB based on SDP1307 SoC - Eval Board";
	compatible = "samsung,stb", "samsung,sdp1307";

	memory {
		device_type = "memory";
		reg = <0x80000000 0x08000000>, <0xc0000000 0x04000000>, <0xe0000000 0x04000000>;
	};

	chosen {
		bootargs = "console=ttyS2,115200N8 root=/dev/mmcblk0p3 rootfstype=squashfs rootwait earlyprintk sdp_sparsemem";
//		bootargs = "console=ttyS2,115200N8 root=/dev/ram0 initrd=0x80800000,8M rootwait earlyprintk sdp_sparsemem";
	};

	fixed-rate-clocks {
		fin {
			compatible = "samsung,sdp-clock-fin";
			clock-frequency = <24000000>;
		};
	};

	internal-micom@0x19200000 {
		compatible = "samsung,golfv-micom";
		reg = <0x19200c80 0x100>, <0x19240000 0x1000>;
		reg-names = "sram", "control";
	};

	emac@18830000 {/* EMAC */
		status = "okay";
	};

	gmac@18860000 {/* GMAC*/
		status = "okay";
	};

	gdma0@0x19060000 {
		status = "okay";
	};

	gdma1@0x19068000 {
		status = "okay";
	};

	sdif@0x180F0000 {
//		status = "okay";
	};

	ssp@0x19070000 {
		status = "okay";
		cs-gpios = <&gpio10 5 0>;/* o_SMC_ADDR[1] port (P10.5) Ctrl reg */

		spidev {
			#address-cells = <1>;
			#size-cells = <1>;
			compatible = "samsung,sdp-spidev";
			spi-max-frequency = <10000000>;
			reg = <0>;
		};
/*
		flash: w25x40@0 {
			#address-cells = <1>;
			#size-cells = <1>;
			compatible = "winbond,w25x40";
			spi-max-frequency = <10000000>;
			reg = <0>;

			partition@0 {
				label = "ucode";
				reg = <0x0 0x80000>;
			};
		};
*/
	};


	serial@18090A00 {
		status = "okay";
	};

	serial@18090A40 {
		status = "okay";
	};

	serial@18090A80 {
		status = "okay";
	};

	i2c@18090100 {
		status = "okay";
		samsung,i2c-sda-delay = <1>;
		samsung,i2c-max-bus-freq = <400000>;
		samsung,i2c-irq-status-reg = <0x180901F4>;
		samsung,i2c-cpu-affinity = <1>;
	};
	
	i2c@18090120 {
		status = "okay";
		samsung,i2c-sda-delay = <1>;
		samsung,i2c-max-bus-freq = <400000>;
		samsung,i2c-irq-status-reg = <0x180901F4>;
		samsung,i2c-cpu-affinity = <1>;
		
		tps56920@37 {
			compatible = "ti,tps56920";
			reg = <0x37>;

			regulator-name = "CPU_PW";
			regulator-min-microvolt = <600000>;
			regulator-max-microvolt = <1870000>;
			regulator-always-on;
			regulator-boot-on;

			default_volt = <1200000>;
		};
	};

	i2c@18090140 { 
		status = "okay";
		samsung,i2c-sda-delay = <1>;
		samsung,i2c-max-bus-freq = <400000>;
		samsung,i2c-irq-status-reg = <0x180901F4>;
		samsung,i2c-cpu-affinity = <1>;

		tps56920@37 {
			compatible = "ti,tps56920";
			reg = <0x37>;

			regulator-name = "GPU_PW";
			regulator-min-microvolt = <600000>;
			regulator-max-microvolt = <1870000>;
			regulator-always-on;
			regulator-boot-on;

			default_volt = <1200000>;
		};
	};

	i2c@18090160 {
		status = "okay";
		samsung,i2c-sda-delay = <1>;
		samsung,i2c-max-bus-freq = <400000>;
		samsung,i2c-irq-status-reg = <0x180901F4>;
		samsung,i2c-cpu-affinity = <1>;

		tps56920@37 {
			compatible = "ti,tps56920";
			reg = <0x37>;

			regulator-name = "CORE_PW";
			regulator-min-microvolt = <600000>;
			regulator-max-microvolt = <1870000>;
			regulator-always-on;
			regulator-boot-on;

			default_volt = <1100000>;
		};
	};

	i2c@18090180 {
		status = "okay";
		samsung,i2c-sda-delay = <1>;
		samsung,i2c-max-bus-freq = <400000>;
		samsung,i2c-irq-status-reg = <0x180901F4>;
		samsung,i2c-cpu-affinity = <1>;
	};

	i2c@180901A0 {
		status = "okay";
		samsung,i2c-sda-delay = <1>;
		samsung,i2c-max-bus-freq = <400000>;
		samsung,i2c-irq-status-reg = <0x180901F4>;
		samsung,i2c-cpu-affinity = <1>;
	};

	i2c@180901C0 {
		status = "okay";
		samsung,i2c-sda-delay = <1>;
		samsung,i2c-max-bus-freq = <400000>;
		samsung,i2c-irq-status-reg = <0x180901F4>;
	//	samsung,i2c-pad-enable = <0x10B00C40 0x8000>;
		samsung,i2c-cpu-affinity = <1>;
	};

	i2c@180901E0 {
		status = "okay";
		samsung,i2c-sda-delay = <1>;
		samsung,i2c-max-bus-freq = <400000>;
		samsung,i2c-irq-status-reg = <0x180901F4>;
	//	samsung,i2c-pad-enable = <0x10B00C40 0x10000>;
		samsung,i2c-cpu-affinity = <1>;

	};

