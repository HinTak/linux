	ehci@0x18800000 {
		status = "okay";
		samsung,sw-reset = <0x18090000 0x8F4 0x1000>;
		samsung,ehci-gpr_reg1 = <0x18850000 0x18 0x002020>;
		samsung,ehci-link-set =  <0x18850000 0x1c 0x8e0000>;
		samsung,sw-reset-release = <0x18090000 0x8F4 0x1000>;
		samsung,vbus-enable = <0x18090000 0xC9C 0x3000 0x08>;
	};

	ehci@0x18810000 {
		status = "okay";
		samsung,sw-reset = <0x18090000 0x8F4 0x2000>;
		samsung,ehci-gpr_reg1 = <0x18850000 0x10 0x002020>;
		samsung,ehci-link-set =  <0x18850000 0x14 0x8e0000>;
		samsung,sw-reset-release = <0x18090000 0x8F4 0x2000>;
//		samsung,vbus-enable = <0x18090000 0xCA0 0x08>;
	};
	
	ehci@0x180E0000 {
		status = "okay";
		samsung,sw-reset = <0x18090000 0x8F4 0x4000>;
		samsung,ehci-gpr_reg1 = <0x180a0000 0x10 0x002020>;
		samsung,ehci-link-set = <0x180a0000 0x14 0x8e0000>;
		samsung,sw-reset-release = <0x18090000 0x8F4 0x4000>;
	};

	
	ohci@0x18808000 {
		status = "okay";
	};

	ohci@0x18818000 {
		status = "okay";
	};
	
	ohci@0x180E8000 {
		status = "okay";
	};

	sata@18870000 {
		status = "okay";
	};

	sata@18880000 {
		status = "okay";
	};
};
