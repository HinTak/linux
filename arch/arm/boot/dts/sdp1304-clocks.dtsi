/ {
	clock: clock-controller@10b00800 {
		ahb_clk: ahb_clk {
			sdp,clock;
			#clock-cells = <0>;
		};
		
		apb_pclk: apb_pclk {
			sdp,clock;
			#clock-cells = <0>;
		};

		emmc_clk: emmc_clk {
			sdp,clock;
			#clock-cells = <0>;
		};

		rstn_uart: rstn_uart {
			sdp,clock;
			#clock-cells = <0>;
		};

		rstn_i2c: rstn_i2c {
			sdp,clock;
			#clock-cells = <0>;
		};
	};

	clock_mp: clock-controller@18090800 {
		disp_pclk_nr: disp_pclk_nr {
