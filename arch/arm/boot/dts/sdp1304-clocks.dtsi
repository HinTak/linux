			#clock-cells = <0>;
		};

		disp_pclk_dcar: disp_pclk_dcar {
			sdp,clock;
			#clock-cells = <0>;
		};

		disp_pclk_ipc: disp_pclk_ipc {
			sdp,clock;
			#clock-cells = <0>;
		};

		disp_pclk_ucar: disp_pclk_ucar {
			sdp,clock;
			#clock-cells = <0>;
		};

		dp_scl_osc_clk: dp_scl_osc_clk {
			sdp,clock;
			#clock-cells = <0>;
		};

		dp_scl_sub_clk: dp_scl_sub_clk {
			sdp,clock;
			#clock-cells = <0>;
		};

		dp_scl_pclk: dp_scl_pclk {
			sdp,clock;
			#clock-cells = <0>;
		};
	};
};
