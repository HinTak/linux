/include/ "sdp.dtsi"
/include/ "sdp1202-pinctrl.dtsi"

/ {
	compatible = "samsung,sdp1202";

	interrupt-parent = <&gic>;

	aliases {
		i2c0 = &i2c_0;
		i2c1 = &i2c_1;
		i2c2 = &i2c_2;
		i2c3 = &i2c_3;
		i2c4 = &i2c_4;
		i2c5 = &i2c_5;
		i2c6 = &i2c_6;
		i2c7 = &i2c_7;
	};

	gic: interrupt-controller@10B81000 {
		compatible = "arm,cortex-a15-gic";
		interrupt-controller;
		#interrupt-cells = <3>;
		reg = <0x10B81000 0x1000>, <0x10B82000 0x1000>;
		cpu-offset = <0x8000>;
		sdp1202-fix = <1>;
	};

	clock: clock-controller@10090800 {
		compatible = "samsung,sdp1202-clock";
		reg = <0x10090800 0x200>;
		#clock-cells = <1>;
	};

	timer: arch-timer {
		compatible = "arm,armv7-timer";
		interrupts = <1 13 0xf08>, 
					 <1 14 0xf08>;		    
	};

	timer@10090400 {
		compatible = "samsung,sdp1202-timer";
		reg = <0x10090400 0x100>;
		interrupts = <0 47 0>;
		clocks = <&clock 15>;
		clock-names = "ahb_clk";
	};

	serial@10090A00 {
		compatible = "samsung,sdp-uart";
		reg = <0x10090A00 0x40>;
		interrupts = <0 43 0>;
		clocks = <&clock 15>, <&clock 88>;
		clock-names = "ahb_clk", "rstn_uart";
		status = "disabled";
	};

	serial@10090A40 {
		compatible = "samsung,sdp-uart";
		reg = <0x10090A40 0x40>;
		interrupts = <0 44 0>;
		clocks = <&clock 15>, <&clock 88>;
		clock-names = "ahb_clk", "rstn_uart";
		status = "disabled";
	};

	serial@10090A80 {
		compatible = "samsung,sdp-uart";
		reg = <0x10090A80 0x40>;
		interrupts = <0 45 0>;
		clocks = <&clock 15>, <&clock 88>;
		clock-names = "ahb_clk", "rstn_uart";
		status = "disabled";
	};

	serial@10090AC0 {
		compatible = "samsung,sdp-uart";
		reg = <0x10090AC0 0x40>;
		interrupts = <0 46 0>;
		clocks = <&clock 15>, <&clock 88>;
		clock-names = "ahb_clk", "rstn_uart";
		status = "disabled";
	};

	pinctrl {
		compatible = "samsung,sdp-pinctrl";
		reg = <0x10090C00 0x300>;
		start-offset = <0xF4>;
		nr-banks = <23>;
	};

	mmc {
		compatible = "samsung,sdp-mmc";
		reg = <0x10000000 0x1000>;
		interrupts = <0 60 0>;
		clocks = <&clock 12>;
		clock-names = "emmc_clk";
		fifo-depth = <0x80>;
		min-max-clock = <400000 50000000>;
		dma-mask = <32>;
		8bit-data;
		highspeed;
		non-removable;
		1.8V-DDR;
		UHS-DDR50;
	};

	i2c_0: i2c@10090100 {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "samsung,sdp-i2c";
		reg = <0x10090100 0x20>;
		interrupts = <0 51 0>;
		clocks = <&clock 89>;
		clock-names = "rstn_i2c";
	};

	i2c_1: i2c@10090120 {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "samsung,sdp-i2c";
		reg = <0x10090120 0x20>;
		interrupts = <0 52 0>;
		clocks = <&clock 89>;
		clock-names = "rstn_i2c";
	};

	i2c_2: i2c@10090140 {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "samsung,sdp-i2c";
		reg = <0x10090140 0x20>;
		interrupts = <0 53 0>;
		clocks = <&clock 89>;
		clock-names = "rstn_i2c";
	};

	i2c_3: i2c@10090160 {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "samsung,sdp-i2c";
		reg = <0x10090160 0x20>;
		interrupts = <0 54 0>;
		clocks = <&clock 89>;
		clock-names = "rstn_i2c";
	};

	i2c_4: i2c@10090180 {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "samsung,sdp-i2c";
		reg = <0x10090180 0x20>;
		interrupts = <0 55 0>;
		clocks = <&clock 89>;
		clock-names = "rstn_i2c";
	};

	i2c_5: i2c@100901A0 {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "samsung,sdp-i2c";
		reg = <0x100901A0 0x20>;
		interrupts = <0 56 0>;
		clocks = <&clock 89>;
		clock-names = "rstn_i2c";
	};

	i2c_6: i2c@100901C0 {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "samsung,sdp-i2c";
		reg = <0x100901C0 0x20>;
		interrupts = <0 57 0>;
		clocks = <&clock 89>;
		clock-names = "rstn_i2c";
	};

	i2c_7: i2c@100901E0 {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "samsung,sdp-i2c";
		reg = <0x100901E0 0x20>;
		interrupts = <0 58 0>;
		clocks = <&clock 89>;
		clock-names = "rstn_i2c";
	};

	ethernet@100D0000 {
		compatible = "samsung,sdp-mac";
		reg = <0x100D0000 0x100>, 
			<0x100D0100 0x300>,
			<0x100D0700 0x100>,
			<0x100D0800 0x100>,
			<0x100D1000 0x100>;
		interrupts = <0 42 0>;
		pad_ctrl_reg = <0x10090CD4 0x1 0x1>;
		bus_width = <64>;
		phy_mask = <0>;
		napi_weight = <128>;
	};

	ehci@100E0000 {
		compatible = "samsung,sdp-ehci", "usb-ehci";
		reg = <0x100E0000 0x100>;
		interrupts = <0 38 0>;
		status = "disabled";
	};

	ehci@100F0000 {
		compatible = "samsung,sdp-ehci", "usb-ehci";
		reg = <0x100F0000 0x100>;
		interrupts = <0 39 0>;
		status = "disabled";
	};

	ohci@100E8000 {
		compatible = "samsung,sdp-ohci", "usb-ohci";
		reg = <0x100E8000 0x100>;
		interrupts = <0 40 0>;
		status = "disabled";
	};

	ohci@100F8000 {
		compatible = "samsung,sdp-ohci", "usb-ohci";
		reg = <0x100F8000 0x100>;
		interrupts = <0 41 0>;
		status = "disabled";
	};
};
