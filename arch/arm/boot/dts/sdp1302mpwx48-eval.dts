/dts-v1/;
/include/ "sdp1302.dtsi"

/ {
	model = "Samsung DTV based on SDP1302 SoC";
	compatible = "samsung,dtv", "samsung,sdp1302";

	memory {
		device_type = "memory";
		reg =  <0x40000000 0x40000000>, <0xc0000000 0x20000000>;
	};

	chosen {
		bootargs = "console=ttyS0,115200N8 root=/dev/mmcblk0p2 rootfstype=squashfs rootwait earlyprintk";
	};

	fixed-rate-clocks {
		fin {
			compatible = "samsung,sdp-clock-fin";
			clock-frequency = <24000000>;
		};
		fin-lvds {
			compatible = "samsung,sdp-clock-fin-lvds";
			clock-frequency = <27000000>;
		};
	};

	chipid@0x10080000 {
		subid = "mpw";
	};

	serial@10090A00 {
		status = "okay";
	};

	serial@10090A40 {
		status = "okay";
	};

	serial@10090A80 {
		status = "okay";
	};

	ethernet@100B0000 {
		phy_sel_reg = <0x10088000 0x1 0x1>;		/* select RGMII for eval board*/
	};

	spi: ssp@0x10090200 {
		status = "okay";
		cs-gpios = <&gpio3 4 0>;/* cs0 is o_CI_RESET Pin(gpio3.4) */

		flash: w25x40@0 {
			#address-cells = <1>;
			#size-cells = <1>;
			compatible = "winbond,w25x40";
			spi-max-frequency = <25000000>;
			reg = <0>;

			partition@0 {
				label = "ucode";
				reg = <0x0 0x400000>;
			};

		};
	};
